     code : |1+
      // Syntax:
      //OnDataChange (<TransactionID>, <NbItems>, <nArrServerHandle>, <vArrValue>, <nArrQuality>, <rArrTimeStamp>)
      //
      // Parameters:
      // 	TransactionID: identifier of the transaction
      // 	NbItems: number of affected items
      // 	nArrServerHandle: array of handles for the affected items
      // 	vArrValue: arrays of values for the affected items
      // 	nArrQuality: arrays of qualities for the transmission
      // 	rArrTimeStamp: arrays of timestamps
      // Return value:
      // 	None
      
      Procedure OnDataChange(TransactionID,NbItems<useful>,nArrServerHandle,vArrValue,nArrQuality,rArrTimeStamp)
      
      // number of the transaction
      Transaction_Change = TransactionID
      // identifier of the item
      Item_Change = nArrServerHandle[1]
      // access to the value of the item
      ReadingValue_Change = vArrValue[1]
      // access to the quality of the transmission
      ReadingQuality_Change = sGetQuality(nArrQuality[1])
      // access to the "timestamp" of the read operation
      dTimeStamp is datetime = rArrTimeStamp[1]
      DateTimeReading_Change = dTimeStamp
      
     type : 458752
