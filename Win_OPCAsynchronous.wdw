   -
     name : Connection
     procedure_id : 1219711952647699681
     type_code : 14
     code : |1+
      // Syntax:
      //Connection ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      Procedure connection()
      
      HourGlass(True)
      
      // connect to the server
      WHEN EXCEPTION IN
      	gautServer>>Connect((gsOPCServer),(gsMachineName))
      DO
      	Error(“Unable to connect”,“Check the connection parameters”)
      	Close()
      END
      // create the group of accesses
      gpautGroup = gautServer>>OPCGroups>>Add(NAME_GROUP)
      IF gpautGroup = Null THEN
      	// the group cannot be created
      	Error(“Unable to create the group ”+NAME_GROUP)
      	RETURN
      END
      // create an access to an item
      gpautItem = gpautGroup>>OPCItems>>AddItem((gsItemID),1)
      IF gpautItem = Null THEN
      	// the item cannot be created
      	Error(“Unable to create the item ”+gsItemID)
      	RETURN
      END
      
      // branch the events on the server
      AutomationEvent(OnServerShutDown,gautServer,GUID_INTERFACE_EVENT_SERVER_AUTOMATION,1)
      
      // branch the events on the groups
      AutomationEvent(OnGlobalDataChange,gautServer>>OPCGroups,GUID_INTERFACE_EVENT_GROUPS_AUTOMATION,1)
      
      // branch the events on the group
      AutomationEvent(OnDataChange,gpautGroup,GUID_INTERFACE_EVENT_GROUP_AUTOMATION,1)
      AutomationEvent(OnAsyncReadComplete,gpautGroup,GUID_INTERFACE_EVENT_GROUP_AUTOMATION,2)
      AutomationEvent(OnAsyncWriteComplete,gpautGroup,GUID_INTERFACE_EVENT_GROUP_AUTOMATION,3)
      gpautGroup>>IsSubscribed = True
      gpautGroup>>IsActive = True
      
      HourGlass(False)
     type : 458752
   -
     name : Disconnection
     procedure_id : 1219711952647765217
     type_code : 14
     code : |1+
      // Syntax:
      //Disconnection ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      Procedure Disconnection()
      
      // end the use of the group
      gautServer>>OPCGroups>>RemoveAll()
      // disconnect from the server
      gautServer>>Disconnect()
      
