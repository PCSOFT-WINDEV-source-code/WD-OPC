     procedure_id : 1219711952647568609
     type_code : 14
     code : |1+
      // Syntax:
      //OnAsyncWriteComplete (<TransactionID>, <NbItems>, <nArrServerHandle>, <nArrError>)
      //
      // Parameters:
      // 	TransactionID: identifier of the transaction
      // 	NbItems: number of affected items
      // 	nArrServerHandle: array of handles for the affected items
      // 	nArrError: arrays of errors in read mode
      // Return value:
      // 	None
      
      Procedure OnAsyncWriteComplete(TransactionID,NbItems <useful>,nArrServerHandle<useful>,nArrError)
      
      // if it's the proper transaction
      IF TransactionID = ID_TRANSACTION_WRITE THEN
      	// process the error case
      	IF nArrError[1]<>0 THEN
      		// the item cannot be read
      		Error(“Unable to read the item ”+gsItemID+“, error #”+nArrError[1])
      		RETURN
      	END
      	// access to the value of the item
      	WritingOK = “OK”
      END
