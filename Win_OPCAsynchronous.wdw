      Procedure Win_OPCAsynchronous(gsMachineName,gsOPCServer,gsItemID)
      
      // instantiate an Automation server
      gautServer is object Automation CLIENT_OPC_AUTOMATION
      gpautGroup is object Automation dynamic
      gpautItem is object Automation dynamic
      
      
      CONSTANT
      	// identifier of transaction in reading
      	READING_TRANSACTION_ID = 1
      	// identifier of transaction in writing
      	ID_TRANSACTION_WRITE = 2
      END
   -
     code : |1+
      // establish the connection
      connection()
