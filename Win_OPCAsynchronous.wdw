     name : OnAsyncReadComplete
     internal_properties : CAAAAAgAAADr4YtsEeH9LhqWghXdCpIruQvbuUmiAJzDDGaOdK+Rl1JsV/qflQc75wkohjfJTiODdezsEbviC8AhkknUu96ohVah4tqailt+5O7G3z0+yw+z+v+usjYKaK5wqpfEzi+SouBy/d7FuQrOMevunzJr98xshmu0dejeiHhIEilQDWe+Yvo6ootMOBpBUDstvbbbhoVWVPEn05jC31VeoDT+FBlpNkQrL0LDaOKeYOsoXuZy
     procedure_id : 1219711952647503073
     type_code : 14
     code : |1+
      // Syntax:
      //OnAsyncReadComplete (<TransactionID>, <NbItems>, <nArrServerHandle>, <vArrValue>, <nArrQuality>, <rArrTimeStamp>, <nArrError>)
      //
      // Parameters:
      //	TransactionID: identifier of the transaction
      //	NbItems: number of affected items
      //	nArrServerHandle: array of handles for the affected items
      //	vArrValue: arrays of values for the affected items
      //	nArrQuality: arrays of qualities for the transmission
      //	rArrTimeStamp: arrays of timestamps
      //	nArrError: arrays of errors in read mode
      // Return value:
      // 	None
      
      Procedure OnAsyncReadComplete(TransactionID,NbItems<useful>,nArrServerHandle<useful>,vArrValue,nArrQuality,rArrTimeStamp,nArrError)
      
      // if it's the proper transaction
      IF TransactionID = READING_TRANSACTION_ID THEN
      	// process the error case
      	IF nArrError[1]<>0 THEN
      		// the item cannot be read
      		Error(“Unable to read the item ”+gsItemID+“, error #”+nArrError[1])
      		RETURN
      	END
      	// access to the value of the item
      	ReadingValue = vArrValue[1]
      	// access to the quality of the transmission
      	ReadingQuality = sGetQuality(nArrQuality[1])
      	// access to the "timestamp" of the read operation
      	dTimeStamp is datetime = rArrTimeStamp[1]
      	DateTimeReading = dTimeStamp
      END
     type : 458752
