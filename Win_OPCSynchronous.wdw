  procedures :
   -
     name : Connection
     procedure_id : 1219711961238881048
     type_code : 14
     code : |1+
      // Syntax:
      //Connection ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procedure connection()
      
      HourGlass(True)
      WHEN EXCEPTION IN
      	// connect to the server
      	gautServer>>Connect((sOPCServer),(sMachineName))
      DO
      	Error(“Unable to connect”,“Check the connection parameters”)
      	Close()
      END
      // create the group of accesses
      gpautGroup = gautServer>>OPCGroups>>Add(NAME_GROUP)
      IF gpautGroup = Null THEN
      	// the group cannot be created
      	Error(“Unable to create the group ”+NAME_GROUP)
      	RETURN
      END
      // create an access to a simple item
      gpautItem = gpautGroup>>OPCItems>>AddItem((sItemID),1)
      IF gpautItem = Null THEN
      	// the item cannot be created
      	Error(“Unable to create the item ”+sItemID)
      	RETURN
      END
      
      HourGlass(False)
      
     type : 458752
   -
     name : Disconnection
     procedure_id : 1219711961238946584
     type_code : 14
     code : |1+
      // Syntax:
      //Disconnection ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      Procedure Disconnection()
      
      // end the use of the group
      gautServer>>OPCGroups>>RemoveAll()
      // disconnect from the server
      gautServer>>Disconnect()
      
     type : 458752
